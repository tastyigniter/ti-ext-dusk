{
  "name": "tastyigniter/ti-ext-dusk",
  "type": "tastyigniter-extension",
  "description": "Laravel Dusk browser testing for TastyIgniter.",
  "license": "MIT",
  "authors": [
    {
      "name": "Sam Poyigi",
      "email": "sam@sampoyigi.com"
    }
  ],
  "require": {
    "laravel/dusk": "^6.15"
  },
  "autoload": {
    "psr-4": {
      "Igniter\\Dusk\\": "src/"
    }
  },
  "extra": {
    "tastyigniter-extension": {
      "code": "igniter.dusk",
      "name": "Dusk",
      "icon": {
        "class": "fa fa-vials",
        "backgroundColor": "#405263",
        "color": "#FFF"
      }
    }
  }
}
